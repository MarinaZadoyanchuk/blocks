{
  "name": "Rodrigez",
  "general": {
    "fontFamily": "'Lato', sans-serif",
    "borderWidth": "2px",
    "textTransform": "uppercase"
  },
  "size": {
    "sm": {
      "padding": "17px 30px",
      "borderRadius": 0,
      "fontSize": "13px"
    },
    "md": {
      "padding": "20px 35px",
      "borderRadius": 0,
      "fontSize": "15px"
    },
    "lg": {
      "padding": "24px 35px",
      "borderRadius": 0,
      "fontSize": "15px"
    }
  },
  "settings": {
    "primary": {
      "default": {
        "bg": {
          "color": ["brand"]
        },
        "color": ["lightShade"],
        "border": {
          "color": ["transparent"]
        }
      },
      "hover": {
        "bg": {
          "color": ["darkShade?lighten=4.71&desaturate=60", "brand?darken=19.02&desaturate=23.83"]
        }
      },
      "active": {
        "bg": {
          "color": ["brand", "brand"]
        }
      }
    },
    "primaryAlt": {
      "default": {
        "bg": {
          "color": ["lightShade?darken=6.27", "lightShade"]
        },
        "color": ["darkShade?lighten=4.71&desaturate=60", "darkShade"],
        "border": {
          "color": ["transparent"]
        }
      },
      "hover": {
        "bg": {
          "color": ["brand", "darkShade?lighten=10.39"]
        },
        "color": ["lightShade", "lightShade"]
      },
      "active": {
        "bg": {
          "color": ["lightShade?darken=6.27", "lightShade"]
        },
        "color": ["darkShade?lighten=4.71&desaturate=60", "darkShade"]
      }
    },
    "secondary": {
      "default": {
        "bg": {
          "color": ["transparent"]
        },
        "color": ["darkShade?lighten=4.71&desaturate=60", "lightShade"],
        "border": {
          "color": ["brand"]
        }
      },
      "hover": {
        "border": {
          "color": ["darkShade?lighten=4.71&desaturate=60", "lightShade"]
        }
      },
      "active": {
        "border": {
          "color": ["brand", "brand"]
        }
      }
    },
    "secondaryAlt": {
      "default": {
        "bg": {
          "color": ["transparent"]
        },
        "color": ["darkShade?lighten=4.71&desaturate=60", "lightShade"],
        "border": {
          "color": ["darkShade?lighten=4.71&desaturate=60", "lightShade"]
        }
      },
      "hover": {
        "border": {
          "color": ["brand", "brand"]
        }
      },
      "active": {
        "border": {
          "color": ["darkShade?lighten=4.71&desaturate=60", "lightShade"]
        }
      }
    }
  }
}
